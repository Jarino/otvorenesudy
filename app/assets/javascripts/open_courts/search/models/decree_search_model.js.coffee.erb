#= require ./search_model

class OpenCourts.DecreeSearchModel extends OpenCourts.SearchModel
  _type:  'decree'

  defaults:
    q:    []
    <% Decree.facets.each do |name, facet| %>
    <%= "#{name}: []" %>
    <% end %>


  query: ->
    super (data) =>
      data.q = @.getQuery() if @.getQuery()?

_.extend(OpenCourts.DecreeSearchModel.prototype.defaults, OpenCourts.SearchModel.prototype.defaults)
