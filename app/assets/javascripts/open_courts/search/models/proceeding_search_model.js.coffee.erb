#= require ./search_model

class OpenCourts.ProceedingSearchModel extends OpenCourts.SearchModel
  _type:  'proceeding'

  defaults:
    <% Proceeding.facets.each do |name, facet| %>
    <%= "#{name}: []" %>
    <% end %>

  query: ->
    super (data) =>
      data.q = @.getQuery() if @.getQuery()?

_.extend(OpenCourts.ProceedingSearchModel.prototype.defaults, OpenCourts.SearchModel.prototype.defaults)
