#= require ./search_model

class OpenCourts.CourtSearchModel extends OpenCourts.SearchModel
  _type:  'court'

  defaults:
    <% Court.facets.each do |name, facet| %>
    <%= "#{name}: []" %>
    <% end %>

  query: ->
    super (data) =>
      data.q = @.getQuery() if @.getQuery()?

_.extend(OpenCourts.CourtSearchModel.prototype.defaults, OpenCourts.SearchModel.prototype.defaults)
