<% @facets.each do |facet| %>
  <% if @collapsible_facet_names.include? facet.name %>
    <% begin %>
    <%= render facet.name, facet: facet, translation: t(facet.key) %>
    <% rescue %>
      <%= render "facets/#{facet.type}_facet", facet: facet, translation: t(facet.key) %>
    <% end %>
  <% end %>
<% end %>
